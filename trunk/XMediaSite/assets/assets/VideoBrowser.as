package assets {		import fl.controls.Button;	import fl.controls.DataGrid;	import fl.controls.TextInput;	import fl.controls.dataGridClasses.DataGridColumn;		import flash.display.MovieClip;		public class VideoBrowser extends MovieClip {				// Constants:		// Public Properties:		// Private Properties:		// UI Elements:		public var data_grid:DataGrid;		public var search_btn:Button;		public var search_txt:TextInput;						///		private var column = new DataGridColumn("Videos")						// Initialization:		public function VideoBrowser() {			configUI();		}		// Public Methods:		// Protected Methods:		// Private Methods:		protected function configUI():void { 			column.cellRenderer = 'VideoBrowserCellRender';			column.headerText	= 'Videos';			column.width = 500;			data_grid.addColumn(column); 		}	}	}