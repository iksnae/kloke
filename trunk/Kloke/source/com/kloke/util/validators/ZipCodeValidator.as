package com.iris.util.validators{	public class ZipCodeValidator	{				static private const ZIPCODE_REGEX : RegExp = /^\d{5}$/		public function ZipCodeValidator()		{		}		static public function validate(zip:String):Boolean{			return isValidZipCode(zip)		}		static private function isValidZipCode(zip : String) : Boolean{            return Boolean(zip.match(ZIPCODE_REGEX));        }	}}